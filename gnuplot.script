set xdata time
set term png
set timefmt '"%Y-%m-%d %H:%M:%S"'
set format x "%H:%M"
set grid ytics xtics
show grid 
set size ratio 1
set output "/var/www/html/saah.png"
set multiplot layout 1,2
set yrange [20:100]
plot "/tmp/saah.log" u 1:2 w l title "humidity"
set yrange [10:35]
plot "/tmp/saah.log" u 1:4 w l title "temperature"
unset multiplot

